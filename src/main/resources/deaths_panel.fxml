<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ul.miage.covid.controller.DeathController">
   <center>
      <LineChart fx:id="deaths_chart" BorderPane.alignment="CENTER">
        <xAxis>
          <CategoryAxis side="BOTTOM" fx:id="x_deaths" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y_deaths" side="LEFT" />
        </yAxis>
      </LineChart>
   </center>
</BorderPane>
