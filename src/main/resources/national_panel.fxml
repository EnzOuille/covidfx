<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.BorderPane?>

<?import javafx.scene.chart.BarChart?>
<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ul.miage.covid.controller.NationalController">
    <center>
        <BarChart fx:id="national_chart" BorderPane.alignment="CENTER">
            <xAxis>
                <CategoryAxis side="BOTTOM" fx:id="x_national" />
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="y_national" side="LEFT" />
            </yAxis>
        </BarChart>
    </center>
</BorderPane>
